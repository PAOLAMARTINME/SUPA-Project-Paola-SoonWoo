<div class="container"></div>
<section id="carousel-bkg">
    <div id="carouselIndex" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselIndex" data-slide-to="0" class="active"></li>
            <li data-target="#carouselIndex" data-slide-to="1"></li>
            <li data-target="#carouselIndex" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="./images/frutasvege.jpeg" alt="First slide">
                <div class="carousel-caption d-none d-md-block">
                    <h2>{{title}}</h2>
                    <p style="color: black ">Descubre nuestros productos ecológicos</p>
                    <a class="carru btn btn-lg" href="/products/list">Ver productos</a>
                </div>
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="./images/vegetales.jpeg" alt="Second slide">
                <div class="carousel-caption d-none d-md-block">
                    <a class="btn btn-lg" href="/products/list">Ver productos</a>
                </div>
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="./images/frutas.jpeg" alt="Third slide">
                <div class="carousel-caption d-none d-md-block">
                    <a class="btn btn-lg" href="/products/list">Ver productos</a>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselIndex" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselIndex" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</section>

<div class="container ">
    <section id="footer" class="row d-flex justify-content-center">
        <footer class="page-footer font-small">
            <div class="col-md-6">

                <input id="origin-input" class="controls" type="text" placeholder="Origen">
                <input id="destination-input" class="controls" type="text" placeholder="Destino" value="madrid">

                <div id="mode-selector" class="controls">
                    <input type="radio" name="type" id="changemode-walking" checked="checked">
                    <label for="changemode-walking">Walking</label>
                    <input type="radio" name="type" id="changemode-transit">
                    <label for="changemode-transit">Transit</label>
                    <input type="radio" name="type" id="changemode-driving">
                    <label for="changemode-driving">Driving</label>
                </div>

                <figure id="myMap"></figure>
                
            </div>

            <div class="footer-copyright text-center py-3">© 2020 Copyright:
                <a href="/"> Paola & SoonWoo</a>

            </div>
        </footer>
    </section>
</div>

